<div class="index_container">

    <% foods.each do |food| %>

        <div class="food_card">
            <img src="<%= food['image_url'] %>" alt="<%= food['food_type']%>">
            <div>
                <h3><%= food['food_type'] %></h3>
                <p> <%= food['location_description'] %> </p>
            </div>
        </div>

    <% end %>

</div>