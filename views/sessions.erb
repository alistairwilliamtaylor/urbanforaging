<p class="current_user_message">You are signed in as <%= current_user['email']%> </p>

<form action="/sessions" class="current_user_message" method="post">
    <input type="hidden" name="_method" value="delete">
    <button>log out</button>
</form>

<div class="banner">
    <h2>Your Posts</h2>
</div>

<a href="/foods/new">add a post</a>

<div class="index_container">

    <% user_foods.each do |food| %>

        <div class="food_card">
            <%= cl_image_tag(food['image_url'], cloud_name: 'dmavnywma', :width => 300, :height => 300, :crop => 'fill', :gravity => 'auto')%>
            <div>
                <h3><a href="/foods?id=<%=food['id']%>" class="main_link"> <%= food['food_type'] %> </a></h3>
                <p> <%= food['location_description'] %> </p>
                <p> posted by: <%= current_user['email'] %> </p>
            </div>
        </div>

    <% end %>

</div>